From 49cf06ec6d95a40179c31c4b8b3baa7c229f0ae4 Mon Sep 17 00:00:00 2001
From: Joshua Riek <jjriek@verizon.net>
Date: Fri, 19 May 2023 15:12:46 -0400
Subject: [PATCH] arm64: dts: remove chosen bootargs for opi5 and opi5b

---
 arch/arm64/boot/dts/rockchip/rk3588s-orangepi-5.dts  | 2 ++
 arch/arm64/boot/dts/rockchip/rk3588s-orangepi-5b.dts | 2 ++
 2 files changed, 4 insertions(+)

diff --git a/arch/arm64/boot/dts/rockchip/rk3588s-orangepi-5.dts b/arch/arm64/boot/dts/rockchip/rk3588s-orangepi-5.dts
index ac8ff3231ff2..d90b128eb17c 100755
--- a/arch/arm64/boot/dts/rockchip/rk3588s-orangepi-5.dts
+++ b/arch/arm64/boot/dts/rockchip/rk3588s-orangepi-5.dts
@@ -14,6 +14,8 @@
 #include "rk3588s-orangepi-5-camera2.dtsi"
 #include "rk3588s-orangepi-5-camera3.dtsi"
 
+/delete-node/ &chosen;
+
 / {
 	model = "Orange Pi 5";
 	compatible = "rockchip,rk3588s-orangepi-5", "rockchip,rk3588";
diff --git a/arch/arm64/boot/dts/rockchip/rk3588s-orangepi-5b.dts b/arch/arm64/boot/dts/rockchip/rk3588s-orangepi-5b.dts
index 2e20a58154d4..a900f6c04956 100755
--- a/arch/arm64/boot/dts/rockchip/rk3588s-orangepi-5b.dts
+++ b/arch/arm64/boot/dts/rockchip/rk3588s-orangepi-5b.dts
@@ -14,6 +14,8 @@
 #include "rk3588s-orangepi-5-camera2.dtsi"
 #include "rk3588s-orangepi-5-camera3.dtsi"
 
+/delete-node/ &chosen;
+
 / {
 	model = "Orange Pi 5B";
 	compatible = "rockchip,rk3588s-orangepi-5", "rockchip,rk3588";
-- 
2.25.1
