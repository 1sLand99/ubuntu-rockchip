From 3be84cb02ddf50ad56412ae0d3c5fb6f589823d4 Mon Sep 17 00:00:00 2001
From: Joshua Riek <jjriek@verizon.net>
Date: Fri, 7 Apr 2023 16:22:11 -0400
Subject: [PATCH] arm64: dts: rockchip: enable dmc

---
 arch/arm64/boot/dts/rockchip/rk3588s-orangepi-5.dts  | 4 ++++
 arch/arm64/boot/dts/rockchip/rk3588s-orangepi-5b.dts | 4 ++++
 2 files changed, 8 insertions(+)

diff --git a/arch/arm64/boot/dts/rockchip/rk3588s-orangepi-5.dts b/arch/arm64/boot/dts/rockchip/rk3588s-orangepi-5.dts
index 4c205970e9..97658ad305 100755
--- a/arch/arm64/boot/dts/rockchip/rk3588s-orangepi-5.dts
+++ b/arch/arm64/boot/dts/rockchip/rk3588s-orangepi-5.dts
@@ -325,6 +325,10 @@ &uart4 {
 	pinctrl-0 = <&uart4m0_xfer>;
 };
 
+&dmc {
+	status = "okay";
+};
+
 &i2c1 {
 	status = "disabled";
 	pinctrl-names = "default";
diff --git a/arch/arm64/boot/dts/rockchip/rk3588s-orangepi-5b.dts b/arch/arm64/boot/dts/rockchip/rk3588s-orangepi-5b.dts
index ef16daed19..650fb9d9a7 100755
--- a/arch/arm64/boot/dts/rockchip/rk3588s-orangepi-5b.dts
+++ b/arch/arm64/boot/dts/rockchip/rk3588s-orangepi-5b.dts
@@ -312,6 +312,10 @@ &uart4 {
 	pinctrl-0 = <&uart4m0_xfer>;
 };
 
+&dmc {
+	status = "okay";
+};
+
 &i2c1 {
 	status = "disabled";
 	pinctrl-names = "default";
-- 
2.25.1

